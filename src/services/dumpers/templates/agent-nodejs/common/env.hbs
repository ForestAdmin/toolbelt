FOREST_AUTH_SECRET={{ forestAuthSecret }}
FOREST_ENV_SECRET={{ forestEnvSecret }}

{{#if forestServerUrl}}
FOREST_SERVER_URL={{ forestServerUrl }}
# This should be removed in production environment.
NODE_TLS_REJECT_UNAUTHORIZED=0

{{/if}}
APPLICATION_PORT={{ appPort }}

DATABASE_URL={{ dbUrl }}
DATABASE_SSL={{ dbSsl }}
# This should be true in production environment. When false, it allows to bypass SSL certificate verification.
DATABASE_REJECT_UNAUTHORIZED=false
{{#if dbSchema }}
DATABASE_SCHEMA={{ dbSchema }}
{{/if}}
{{#if hasDockerDbUrl}}
DOCKER_DATABASE_URL={{ dockerDbUrl }}
{{/if}}

NODE_ENV=development

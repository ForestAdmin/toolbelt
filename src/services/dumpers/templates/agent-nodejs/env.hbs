FOREST_AUTH_SECRET={{ forestAuthSecret }}
FOREST_ENV_SECRET={{ forestEnvSecret }}

FOREST_SERVER_URL={{ forestServerUrl }}
# This should be removed in production environment.
NODE_TLS_REJECT_UNAUTHORIZED=0

APPLICATION_PORT={{ applicationPort }}

DATABASE_URL={{ databaseUrl }}
DATABASE_SSL={{ databaseSsl }}
# This should be true in production environment. It allows to bypass SSL certificate verification.
DATABASE_REJECT_UNAUTHORIZED=true
{{#if dbSchema }}
DATABASE_SCHEMA={{ databaseSchema }}
{{/if}}
{{#if hasDockerDatabaseUrl}}
DOCKER_DATABASE_URL={{ dockerDatabaseUrl }}
{{/if}}

NODE_ENV=development
